<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="SwipeBackLayoutStyle" format="reference" />
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CellView">
        <attr name="cell_icon" format="reference" />
        <attr name="cell_icon_width" format="dimension" />
        <attr name="cell_icon_visible" format="reference|boolean" />
        <attr name="cell_icon_height" format="dimension" />
        <attr name="cell_name" format="reference|string" />
        <attr name="cell_name_size" format="dimension" />
        <attr name="cell_name_color" format="color" />
        <attr name="cell_content" format="reference|string" />
        <attr name="cell_content_size" format="dimension" />
        <attr name="cell_content_color" format="color" />
        <attr name="cell_contentleft" format="reference|string" />
        <attr name="cell_contentleft_size" format="dimension" />
        <attr name="cell_contentleft_color" format="color" />
        <attr name="cell_arrow_visible" format="reference|boolean" />
        <attr name="cell_rightimage_visible" format="reference|boolean" />

    </declare-styleable>

    <declare-styleable name="CellEditView">
        <attr name="celledit_name" format="reference|string" />
        <attr name="celledit_name_size" format="dimension" />
        <attr name="celledit_name_color" format="color" />
        <attr name="celledit_hint" format="reference|string" />
        <attr name="celledit_cashall" format="reference|boolean" />
        <attr name="celledit_right_visible" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="WPEditText">
        <attr name="edit_icon" format="reference|integer" />
        <attr name="edit_left_text" format="reference|string" />
        <attr name="edit_show_eye" format="reference|boolean" />
        <attr name="edit_hint" format="reference|string" />
        <attr name="edit_pwd" format="reference|boolean" />
        <attr name="edit_show_icon" format="reference|boolean" />
        <attr name="edit_show_right_text" format="reference|boolean" />
        <attr name="edit_right_text" format="reference|string" />
    </declare-styleable>

    <declare-styleable name="DotView">
        <attr name="dot_radius" format="dimension" />
        <attr name="dot_span" format="dimension" />
        <attr name="dot_unselected_color" format="reference" />
        <attr name="dot_selected_color" format="reference" />
    </declare-styleable>

    <declare-styleable name="SliderBanner">
        <attr name="slider_banner_indicator" format="reference" />
        <attr name="slider_banner_pager" format="reference" />
        <attr name="slider_banner_time_interval" format="integer" />
    </declare-styleable>

    <declare-styleable name="IndexRankingItem">
        <attr name="index_ranking_item_title" format="reference|string" />
        <attr name="index_ranking_item_description" format="reference|string" />
        <attr name="index_ranking_item_icon" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference"/>
        <attr name="mvAnimDuration" format="integer|reference"/>
        <attr name="mvTextSize" format="dimension|reference"/>
        <attr name="mvTextColor" format="color|reference"/>
        <attr name="mvSingleLine" format="boolean"/>
        <attr name="mvGravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSeekBar">
        <attr name="numTextFormat" format="string"/>
        <attr name="numbackground" format="reference"/>
        <attr name="numTextSize" format="dimension"/>
        <attr name="numTextColor" format="color"/>
        <attr name="numScale" format="string"/>
        <attr name="numType" format="enum">
            <enum name="top" value="48"/>
            <enum name="bottom" value="80"/>
        </attr>

    </declare-styleable>
</resources>